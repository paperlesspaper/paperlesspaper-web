  "scripts": {
    "start": "env-cmd -f .env SASS_SILENCE_DEPRECATIONS=all VITE_CJS_TRACE=true vite dev",
    "dev": "SASS_SILENCE_DEPRECATIONS=all VITE_CJS_TRACE=true vite dev",
    "dev:prod": "env-cmd -f .env.production SASS_SILENCE_DEPRECATIONS=all VITE_CJS_TRACE=true vite dev",
    "start:local-https": "export HTTPS=true&&SSL_CRT_FILE=./cert/cert.pem&&SSL_KEY_FILE=.cert/key.pem env-cmd -f .env.local env-cmd -f .env.localhttps react-scripts start",
    "start:production": "env-cmd -f .env.production tsc && vite build",
    "build:local": "CI=false env-cmd -f .env tsc && vite build",
    "build:production": "CI=false GENERATE_SOURCEMAP=false env-cmd -f .env.production tsc && vite build",
    "deploy:app": "npm run resources && npm run build:app && npm run deploy:googleplay && npm run deploy:testflight",
    "deploy:testflight": "cd ios/App && env-cmd -f ../../.env.production bundle exec fastlane beta",
    "deploy:appstore": "cd ios/App && APP_STORE_RELEASE_NOTES_FILE=../../release-notes.txt env-cmd -f ../../.env.production bundle exec fastlane release",
    "deploy:googleplay": "cd android && env-cmd -f ../.env.production bundle exec fastlane internal",
    "deploy:playstore": "cd android && env-cmd -f ../.env.production bundle exec fastlane production",
    "build": "tsc && vite build",
    "build:app": "npm run build:production && npx cap sync",
    "preview": "vite preview",
    "wirewire:deploy:app": "CI=false env-cmd -f .env.wirewire npm run wirewire:resources && node ./scripts/update_ios_deployment.cjs && npm run wirewire:build:app && npm run wirewire:deploy:googleplay && npm run wirewire:deploy:testflight",
    "wirewire:deploy:productionapp": "CI=false env-cmd -f .env.wirewire npm run wirewire:resources && node ./scripts/update_ios_deployment.cjs && npm run wirewire:build:app && npm run wirewire:deploy:playstore && npm run wirewire:deploy:appstore",
    "wirewire:build:production": "CI=false tsc && vite build --mode wirewire",
    "wirewire:build:app": "npm run wirewire:build:production && NODE_ENV=wirewire npx cap sync",
    "wirewire:dev": "SASS_SILENCE_DEPRECATIONS=all VITE_CJS_TRACE=true vite dev --mode local.wirewire",
    "wirewire:deploy:testflight": "cd ios/App && env-cmd -f ../../.env.wirewire bundle exec fastlane beta",
    "wirewire:deploy:googleplay": "cd androidWirewire && env-cmd -f ../.env.wirewire bundle exec fastlane internal",
    "wirewire:deploy:appstore": "cd ios/App && APP_STORE_RELEASE_NOTES_FILE=../../release-notes.txt env-cmd -f ../../.env.wirewire bundle exec fastlane release",
    "wirewire:deploy:playstore": "cd androidWirewire && env-cmd -f ../.env.wirewire bundle exec fastlane production",
    "wirewire:resources": "NODE_ENV=wirewire npx @capacitor/assets generate --assetPath ./resourcesWirewire --androidProject ./androidWirewire --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'",
    "wirewire:run:app": "env-cmd -f .env.wirewire ionic cap run --source-map",
    "wirewire:run:app:live": "env-cmd -f .env.wirewire ionic cap run --livereload",
    "wirewire:run:ios": "env-cmd -f .env.wirewire tsc && vite build --mode wirewire && NODE_ENV=wirewire npx cap run ios",
    "wirewire:run:android": "env-cmd -f .env.wirewire tsc && vite build --mode wirewire && NODE_ENV=wirewire npx cap run android",
    "run:app": "env-cmd -f .env.production ionic cap run --source-map",
    "run:android": "tsc && vite build && npx cap run android",
    "run:ios": "tsc && vite build && npx cap run ios",
    "run:app:live": "env-cmd -f .env.production ionic cap run --livereload",
    "resources": "npx @capacitor/assets generate --assetPath ./resources --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'",
    "sentry:release": "(export REACT_APP_SENTRY_RELEASE=$(git rev-parse --short HEAD); node scripts/sentrySourcemap.js)",
    "generate:figma": "env-cmd -f .env.local wfp-cli figma-colors src/themes/index.js --name memo --output ./src/themes",
    "generate:figmadark": "env-cmd -f .env.dark wfp-cli figma-colors src/themes/index.js --name memodark --output ./src/themes/dark",
    "generate:theme": "wfp-cli theme src/themes/index.js --name CarbonThemes --source ./lib --output ./src/themes/output/generated",
    "test": "react-scripts test",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "link:un": "yarn link @progressiveui/react && yarn link @progressiveui/styles && yarn link @progressiveui/themes-core",
    "unlink:un": "yarn unlink @progressiveui/react && yarn unlink @progressiveui/styles && yarn unlink @progressiveui/themes-core && yarn add @progressiveui/react @progressiveui/styles @progressiveui/themes-core && yarn install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
