  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "coveralls": "^3.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-security": "^3.0.1",
    "faker": "^6.6.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "vitest": "^4.0.18"
  }
}
