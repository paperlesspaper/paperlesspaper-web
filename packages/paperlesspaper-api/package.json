{
  "name": "@paperlesspaper/api",
  "version": "1.224.2",
  "private": true,
  "description": "Paperlesspaper API backend",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production PORT=3000 node --loader ts-node/esm --experimental-specifier-resolution=node src/index.ts",
    "deploy": "fly deploy",
    "dev": "NODE_OPTIONS=--preserve-symlinks NODE_ENV=development PORT=5002 nodemon src/index.ts",
    "dev:limit": "NODE_ENV=development PORT=5002 nodemon --exec \"node --max-old-space-size=800 -r ts-node/register\" src/index.ts",
    "dev:prod": "NODE_ENV=production PORT=5002 env-cmd -f .env.production nodemon src/index.ts",
    "dev:https": "NODE_ENV=development PORT=443 nodemon src/index-https.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:smoke": "vitest run tests/smoke",
    "stripe:listen": "stripe listen --forward-to localhost:5002/webhook",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.2",
    "@aws-sdk/client-cloudwatch-events": "^3.216.0",
    "@aws-sdk/client-s3": "^3.216.0",
    "@aws-sdk/client-sesv2": "^3.328.0",
    "@aws-sdk/credential-provider-node": "^3.830.0",
    "@aws-sdk/lib-storage": "^3.216.0",
    "@aws-sdk/s3-request-presigner": "^3.216.0",
    "@aws-sdk/util-endpoints": "^3.216.0",
    "@internetderdinge/api": "1.229.0",
    "@sentry/node": "^10.25.0",
    "@sentry/profiling-node": "^10.25.0",
    "agenda": "^5.0.0",
    "agendash": "^4.0.0",
    "auth0": "^4.23.1",
    "aws-crt": "^1.27.3",
    "aws-iot-device-sdk-v2": "^1.22.0",
    "aws-sdk": "^2.1261.0",
    "aws4": "^1.11.0",
    "axios": "^1.9.0",
    "body-parser": "~1.15.0",
    "body-parser-xml": "~1.1.0",
    "canvas": "^3.1.0",
    "compression": "^1.8.0",
    "config": "^4.0.0",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "2.0.0",
    "dotenv": "^10.0.0",
    "elevenlabs": "^0.5.0",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-ws": "^5.0.2",
    "firebase-admin": "^11.0.0",
    "googleapis": "^144.0.0",
    "he": "^1.2.0",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "https": "^1.0.0",
    "i18next": "^23.10.1",
    "joi": "^17.3.0",
    "js-yaml": "^3.8.4",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.3",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.34",
    "mongoose": "^8.15.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
