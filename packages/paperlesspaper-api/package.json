{
  "name": "@paperlesspaper/api",
  "version": "1.224.2",
  "private": true,
  "description": "Paperlesspaper API backend",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production PORT=3000 TS_NODE_SKIP_IGNORE=true node --loader ts-node/esm --experimental-specifier-resolution=node src/index.ts",
    "deploy": "node ./scripts/deploy-with-live-internetderdinge-api.mjs -- fly deploy",
    "dev": "NODE_OPTIONS=--preserve-symlinks NODE_ENV=development PORT=5002 nodemon src/index.ts",
    "dev:limit": "NODE_ENV=development PORT=5002 nodemon --exec \"node --max-old-space-size=800 -r ts-node/register\" src/index.ts",
    "dev:prod": "NODE_ENV=production PORT=5002 env-cmd -f .env.production nodemon src/index.ts",
    "dev:https": "NODE_ENV=development PORT=443 nodemon src/index-https.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:smoke": "vitest run tests/smoke",
    "stripe:listen": "stripe listen --forward-to localhost:5002/webhook",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.0.0",
    "@aws-sdk/client-cloudwatch-events": "^3.989.0",
    "@aws-sdk/client-s3": "^3.989.0",
    "@aws-sdk/client-sesv2": "^3.989.0",
    "@aws-sdk/credential-provider-node": "^3.972.8",
    "@aws-sdk/lib-storage": "^3.989.0",
    "@aws-sdk/s3-request-presigner": "^3.989.0",
    "@aws-sdk/util-endpoints": "^3.989.0",
    "@internetderdinge/api": "file:.yalc/@internetderdinge/api",
    "@paperlesspaper/helpers": "^1.223.0",
    "@sentry/node": "^10.38.0",
    "@sentry/profiling-node": "^10.38.0",
    "agenda": "^6.2.1",
    "agendash": "^8.1.0",
    "auth0": "^5.3.1",
    "aws-crt": "^1.29.0",
    "aws-iot-device-sdk-v2": "^1.25.0",
    "aws-sdk": "^2.1693.0",
    "aws4": "^1.13.2",
    "axios": "^1.13.5",
    "body-parser": "~2.2.2",
    "body-parser-xml": "~2.0.5",
    "canvas": "^3.2.1",
    "compression": "^1.8.1",
    "config": "^4.3.0",
    "cors": "^2.8.6",
    "cron": "^4.4.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "3.2.0",
    "dotenv": "^17.3.0",
    "elevenlabs": "^1.59.0",
    "express": "^5.2.1",
    "express-jwt": "^8.5.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-ws": "^5.0.2",
    "firebase-admin": "^13.6.1",
    "googleapis": "^171.4.0",
    "he": "^1.2.0",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "https": "^1.0.0",
    "i18next": "^25.8.6",
    "joi": "^18.0.2",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "mongoose": "^8.15.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "multiparty": "~4.2.3",
    "node-uuid": "~1.4.8",
    "nodemailer": "^8.0.1",
    "openai": "^6.21.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "path": "~0.12.7",
    "pixelmatch": "^7.1.0",
    "promise": "~8.3.0",
    "puppeteer": "^24.37.2",
    "qs": "^6.14.2",
    "request": "^2.88.2",
    "rrule": "^2.8.1",
    "sharp": "^0.34.5",
    "stripe": "^20.3.1",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "validator": "^13.15.26",
    "winston": "^3.19.0",
    "xss-clean": "^0.1.4",
    "zod": "^4.3.6"
